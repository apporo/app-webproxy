{
  "rules": [
    {
      "id": "elasticsearch-plugins",
      "enabled": true,
      "source": {
        "url": "/tool(.*)",
        "methods": ["GET", "PUT"]
      },
      "target": {
        "hostname": "192.168.56.56",
        "port": 9200,
        "url": "$1",
        "headers": {
          "accept-encoding": "*;q=1,gzip=0"
        }
      }
    }
  ]
}
